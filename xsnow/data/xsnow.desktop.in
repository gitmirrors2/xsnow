[Desktop Entry]
Name=xsnow
Comment=Snow, Santa and birds on your desktop
Exec=xsnow
Icon=xsnow
Type=Application
Categories=Game;
Terminal=false
Name[en_US]=xsnow
Keywords=christmas;santa;snow;xmas
Version=XSNOW_VERSION
StartupNotify=true
Actions=start;nomenu;

[Desktop Action start]
Name=Start xsnow
Exec=xsnow

[Desktop Action nomenu]
Name=Start without menu
Exec=xsnow -hidemenu
